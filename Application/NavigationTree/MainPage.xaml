<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:mainApp="clr-namespace:Application.NavigationTree.MainApp"
             x:Class="Application.NavigationTree.MainApp.MainPage">
    <ContentPage.BindingContext>
        <mainApp:MainPageViewModel />
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout>
            <Button Command="{Binding BrCommand}" Text="Leaderboard"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>